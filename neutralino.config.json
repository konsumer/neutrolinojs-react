{
  "applicationId": "js.neutralino.sample",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/frontend/",
  "url": "/frontend/",
  "enableServer": false,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/frontend/icon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    }
  },
  "cli": {
    "binaryName": "demo",
    "resourcesPath": "/frontend/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/frontend/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}